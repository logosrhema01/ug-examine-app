{
  "name": "ug-examine-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npx tailwindcss -i ./src/styles.css -o ./dist/ug-examine-app/output.css",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gen:staff:main:api": "openapi-generator-cli generate -i ./main.openapi.yaml -g typescript-angular -p=removeOperationIdPrefix=true  -o projects/staff/src/app/api/main/v1",
    "gen:student:main:api": "openapi-generator-cli generate -i ./main.openapi.yaml -g typescript-angular -p=removeOperationIdPrefix=true  -o projects/student/src/app/api/main/v1"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.1",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.1",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@fullcalendar/angular": "^6.1.8",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@popperjs/core": "^2.9.1",
    "@tailwindcss/forms": "^0.5.5",
    "bootstrap": "^5.2.3",
