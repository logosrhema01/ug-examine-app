    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/jasmine": "~4.3.0",
    "autoprefixer": "^10.4.15",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
